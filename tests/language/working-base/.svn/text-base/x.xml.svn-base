         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <procedure name="q" src="line 1">

      <output 
    name="o" type="int"

     xsi:nil="true" />

      <input 
    name="i" type="int"

     xsi:nil="true" />
