<program xmlns="http://ci.uchicago.edu/swift/2009/02/swiftscript"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns:xs="http://www.w3.org/2001/XMLSchema">
  
  <types>
     	<type>
     		<typename>file</typename>
     		<typealias>string</typealias>
     		<typestructure></typestructure>
     	</type>   
  </types>  
  <procedure name="echo" src="line 2">

      <output 
    name="t" type="file"

     xsi:nil="true" />

      <input 
    name="s" type="string"

     xsi:nil="true" />
    <binding>
      <application src="line 3">
        <executable>echo</executable>
        <stdout><function name="filename">
          <variableReference>t</variableReference></function></stdout><variableReference>s</variableReference>  </application>
    </binding>
  </procedure>
  <variable name="hw" type="file" isGlobal="false" xsi:nil="true"/>
  <call proc="echo" src="line 8">
    <output><variableReference>hw</variableReference></output>
    <input><stringConstant>hello world</stringConstant></input>
  </call>
</program>
